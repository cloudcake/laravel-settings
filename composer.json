{
  "name": "larashim/settings",
  "description": "Associate generic data to any other model without polluting your application model logic.",
  "type": "library",
  "license": "MIT",
  "authors": [{
    "name": "Stephen Lake",
    "email": "stephen-lake@live.com"
  }],
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "laravel/framework": "~5.5.0|~5.6.0|~5.7.0",
    "orchestra/testbench": "~3.4.0|~3.5.0|~3.6.0"
  },
  "autoload": {
    "psr-4": {
      "Larashim\\Settings\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Larashim\\Settings\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Larashim\\Settings\\Providers\\SettingsServiceProvider"
      ]
    }
  }
}
